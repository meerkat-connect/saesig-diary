<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="saesigDiary.member.MemberMapper">
    <select id="findAll" resultType="saesigDiary.member.MemberDto">
        SELECT
            id
            , email
            , password
        FROM member
    </select>

    <select id="findByEmail" resultType="saesigDiary.member.MemberDto">
        SELECT
            id
            , email
            , password
        FROM member
        WHERE email = #{email}
    </select>
</mapper>